# Anthropic Claude API
# Get your API key from: https://console.anthropic.com/
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Qdrant Vector Database
# Get your credentials from: https://cloud.qdrant.io/
QDRANT_URL=https://your-cluster.qdrant.io
QDRANT_API_KEY=your_qdrant_api_key_here

# Supabase (PostgreSQL)
# Get your credentials from: https://supabase.com/dashboard
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_anon_key_here

# Redis (Session State)
# Get your credentials from your Redis provider
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=your_redis_password_here

# ElevenLabs (Voice - Optional)
# Get your API key from: https://elevenlabs.io/
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# PredictHQ (Events - Optional)
# Get your API key from: https://www.predicthq.com/
PREDICTHQ_API_KEY=your_predicthq_api_key_here

# Weather API (Optional)
# Get your API key from your weather provider
WEATHER_API_KEY=your_weather_api_key_here

# Mistral AI
# Get your API key from: https://console.mistral.ai/
MISTRAL_API_KEY=your_mistral_api_key_here
